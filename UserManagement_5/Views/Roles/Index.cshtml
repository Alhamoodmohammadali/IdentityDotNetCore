@model IEnumerable<IdentityRole>
@{
    ViewData["Title"] = "Roles";
}

<h1>Roles </h1>
<table class="table table-striped mt-4">
    <theade>
        <tr class="bg-primary text-white">  </tr>
    </theade>
    <tbody>
        @foreach (var Role in Model)
        {
            <tr>
                <td>@Role.Id</td>
                <td>@Role.Name</td>
            </tr>
        }
    </tbody>
</table>